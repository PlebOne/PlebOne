# Production Environment Variables for PlebOne
# Copy this file to .env.prod and update the values

# Domain configuration
DOMAIN=pleb.one

# Database configuration - CHANGE THIS TO A SECURE PASSWORD
DB_PASSWORD=your_secure_database_password_here

# Admin authentication - Nostr public key in hex format
# This is already set to your provided nsec's public key
ADMIN_PUBKEYS=8dc8688200b447ec2e4018ea5e42dc5d480940cb3f19ca8f361d28179dc4ba5e

# Application configuration
NODE_ENV=production
PORT=3000